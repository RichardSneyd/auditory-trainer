{
  "manifest_version": 3,
  "short_name": "ListenUp",
  "name": "ListenUp: Free Listening Therapy",
  "description": "A free auditory integration &amp; brainwave entrainment tool!",
  "version": "1",
  "icons": [
    {
      "src": "icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png"
    },
    {
      "src": "icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "start_url": "./index.html",
  "background_color": "#FFFFFF",
  "display": "standalone",
  "scope": "/",
  "theme_color": "#000000",
  "id": "listen_up",
  "dir": "auto",
  "lang": "en",
  "orientation": "any",
  "display_override": [
    "window-controls-overlay",
    "standalone"
  ],
  "categories": [
    "education",
    "health",
    "kids",
    "medical",
    "music"
  ]
}